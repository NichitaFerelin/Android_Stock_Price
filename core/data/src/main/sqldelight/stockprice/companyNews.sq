CREATE TABLE CompanyNewsDBO(
    id INTEGER NOT NULL PRIMARY KEY,
    companyId INTEGER AS Int NOT NULL,
    headline TEXT NOT NULL,
    source TEXT NOT NULL,
    sourceUrl TEXT NOT NULL,
    summary TEXT NOT NULL,
    dateMillis INTEGER NOT NULL
);

insert:
INSERT OR REPLACE INTO CompanyNewsDBO VALUES ?;

getAllBy:
SELECT * FROM CompanyNewsDBO WHERE companyId = :companyId;

eraseAllBy:
DELETE FROM CompanyNewsDBO WHERE companyId = :companyId;